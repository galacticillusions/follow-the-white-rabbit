{
  "version": 2,
  "env": {
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_ANON_KEY": "@supabase-anon-key",
    "SENDGRID_API_KEY": "@sendgrid-key",
    "STRIPE_SECRET_KEY": "@stripe-secret"
  },
  "build": {
    "env": {
      "NODE_VERSION": "18"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
